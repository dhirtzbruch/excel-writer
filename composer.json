{
    "name": "sleussink/excel-writer",
    "type": "library",
    "description": "A library for simple creation of excel-files",
    "keywords": [
        "sleussink",
        "ExcelWriter"
    ],
    "homepage": "https://github.com/sleussink/ExcelWriter",
    "license": "MIT",
    "authors": [
        {
            "name": "Steffen Leu√üink",
            "email": "slk@fastbolt.com",
            "homepage": "https://github.com/sleussink",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.4",
        "phpoffice/phpspreadsheet": "^1.20"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.3",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0",
        "vimeo/psalm": "^4.17"
    },
    "autoload": {
        "psr-4": {
            "Sleussink\\ExcelWriter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sleussink\\ExcelWriter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "prefer-stable": true
}
